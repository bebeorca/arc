{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static-build"
      },
      {
        "src": "artisan",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }
  